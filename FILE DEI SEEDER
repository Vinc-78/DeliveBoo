            for( $i = 0; $i < 10; $i++ ) {
                   $newPlate = new Plate();
                   $newPlate->restaurant_id = 1;
                   $newPlate-category_id = rand(1, 5);
                   $newPlate->name = $names[$i];
                   $newPlate->description = $descriprions[$i];
                   $newPlate->price = rand(10, 500) / 10;
                   $newPlate->image_url();
                   $newPlate->visibility = true;
                   $newPlate->save();
