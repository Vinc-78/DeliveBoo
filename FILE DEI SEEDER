//SEEDER CATEGORY

public function run() {
$restaurantCategory = [

[
    'id' => 1,
    'name' => 'Italian',
    'image_url' => 'italiano.jpg'
    
],
[
    'id' => 2,
    'name' => 'Chinese',
    'image_url' => 'orientale.jpg'
    
],
[
    'id' => 4,
    'name' => 'Fish',
    'image_url' => 'pesce.jpg'
    
],
[
    'id' => 5,
    'name' => 'Vegetarian',
    'image_url' => 'vegano'
    
],
[
    'id' => 6,
    'name' => 'Indian',
    'image_url' => 'indiano'
    
],
[
    'id' => 7,
    'name' => 'Chicken',
    'image_url' => 'carne.jpg'
    
],
[
    'id' => 8,
    'name' => 'Pizza',
    'image_url' => 'pizza.jpg'
    
],
[
    'id' => 9,
    'name' => 'Dessert',
    'image_url' => 'dessert.jpg'
    
],
[
    'id' => 10,
    'name' => 'Burgers',
    'image_url' => 'hamburger.jpg'
   
],
],

foreach($restaurantCategory as $category) {
            $newRestaurantsType = new RestaurantType();
            $newRestaurantsType->name = $category['name'];
            $newRestaurantsType->imge_url = $category['img'];

            $newRestaurantsType->save();
        }
}


